{
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchDatasources": [
        "docker",
        "apk"
      ],
      "matchUpdateTypes": [
        "digest",
        "minor",
        "patch",
        "major"
      ],
      "branchName": "renovate/auto-updates",
      "prPriority": 1,
      "matchPackageNames": [
        "/snapcast/",
        "/librespot/"
      ]
    }
  ]
}
